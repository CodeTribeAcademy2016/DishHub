<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.restauarant.codetribe.disapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".navigation"
            android:label="@string/title_activity_navigation"
            android:theme="@style/AppTheme" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".Favourites" />
        <activity android:name=".Splash" />
        <activity
            android:name=".Developers"
            android:label="@string/title_activity_developers"
            android:theme="@style/AppTheme" />
        <activity android:name=".Menu" />
        <activity
            android:name="com.example.codetribe.disapp.CustomerNavigation"
            android:label="@string/title_activity_customer_navigation"
            android:theme="@style/AppTheme" />
        <activity android:name="com.example.codetribe.disapp.ChangePassword"></activity>
    </application>

</manifest>

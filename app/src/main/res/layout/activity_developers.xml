<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#A5D6A7"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.codetribe.disapp.Developers"
    tools:showIn="@layout/activity_developers">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView6"
        android:src="@drawable/n"
        android:layout_above="@+id/imageView9"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/justice"

        android:id="@+id/textView2"
        android:layout_alignTop="@+id/imageView6"
        android:layout_toEndOf="@+id/imageView6"
        android:textColor="#080808" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView7"
        android:src="@drawable/muz"
        android:layout_marginBottom="87dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/muzi"

        android:id="@+id/textView3"
        android:layout_alignBottom="@+id/imageView7"
        android:layout_alignStart="@+id/textView2"
        android:textColor="#0c0c0c" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView8"
        android:src="@drawable/m"
        android:layout_above="@+id/imageView7"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/Lerato"

        android:id="@+id/textView4"
        android:layout_below="@+id/imageView9"
        android:layout_alignStart="@+id/textView2"
        android:textColor="#0c0c0c" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView9"
        android:src="@drawable/s"
        android:layout_above="@+id/imageView8"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/saviour"

        android:id="@+id/textView5"
        android:layout_alignTop="@+id/imageView9"
        android:layout_alignStart="@+id/textView2"
        android:textColor="#0b0b0b" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="DEVELOPERS"
        android:id="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView2"
        android:textColor="#060606" />


</RelativeLayout>
